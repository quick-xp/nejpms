<h1>在庫マスタ</h1>
<%= form_for @asset_list_masters, url: asset_list_masters_path, method: :post do |f| %>

<div class="field">
  <%= f.label :station_id %><br />
  <%= f.collection_select :station_id,  @select_stations, :station_id, :station_name %>
</div>

<div class="actions">
  <%= f.submit("選択") %>
</div>
<% end %>

